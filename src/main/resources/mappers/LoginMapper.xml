<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"
        >
<mapper namespace="com.ou.mapper.LoginMapper">

    <select id="querylogin" resultType="Login">
        select username,password from ou.login;
    </select>

    <select id="queryAll" resultType="Login">
        select * FROM ou.login;
    </select>


</mapper>